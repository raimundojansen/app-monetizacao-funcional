{
  "name": "app-monetizacao-funcional",
  "version": "1.0.0",
  "description": "Aplicativo completo para monetização - geração de conteúdo, automação, pagamentos e análise",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack --mode production",
    "build:server": "babel src --out-dir dist",
    "test": "jest",
    "deploy": "vercel --prod"
  },
  "keywords": ["monetizacao", "automacao", "pagamentos", "analytics", "content-generation"],
  "author": "raimundojansen",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "axios": "^1.6.0",
    "stripe": "^13.10.0",
    "openai": "^4.20.0",
    "mongoose": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "node-cron": "^3.0.3",
    "puppeteer": "^21.5.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
